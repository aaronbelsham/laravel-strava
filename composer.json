{
    "name": "aaronbelsham/laravel-strava",
    "description": "A laravel package to access data from the Strava API.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AaronBelsham\\Strava\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronBelsham\\Strava\\StravaServiceProvider"
            ],
            "aliases": {
                "Strava": "AaronBelsham\\Strava\\StravaFacade"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Forked from Richie McMullen",
            "email": "richiemcmullen007@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
